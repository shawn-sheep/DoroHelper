        MsgBox("该版本有重大调整，所有设置将重置")
        FileDelete "settings.ini"
        ; 使用之前保存的副本恢复默认设置
        g_settings := g_default_settings.Clone()
        g_numeric_settings := g_default_numeric_settings.Clone()
